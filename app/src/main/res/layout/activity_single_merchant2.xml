<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="demo.projectZero.TargetActivity">

    <ScrollView
        android:layout_width="380dp"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="商店名称"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2Name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView55"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="经营地址"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"
                android:clickable="true"/>

            <TextView
                android:id="@+id/merchant2Address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView57"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="经营者姓名"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"  />

            <TextView
                android:id="@+id/merchant2peopleInCharge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView59"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="经营者籍贯"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2Home"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView45"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="联系电话"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2PhoneNum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"
                android:autoLink="phone"
                android:textColorLink="@color/primary_darker"/>

            <TextView
                android:id="@+id/textView49"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="无证经营开始时间"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2StartTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView51"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="登记时间"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2RegisterTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView53"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="无证经营现状"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2CurrentStatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="变化时间"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2ChangeTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否有工商营业执照"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2CommercialLicense"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否位于中小学校周边100米内"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2NearSchool"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />
            <TextView
                android:id="@+id/textView2333"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否属于拆迁或待拆迁区域"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2Relocation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />
            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否具有办证意愿"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2Willingness"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="经营场所性质"
                android:textColor="@color/mb_red500"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/merchant2LocationProperty"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="是否能够办证"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"  />

            <TextView
                android:id="@+id/merchant2AbleToApply"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView75"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="卷烟进货渠道"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"  />

            <TextView
                android:id="@+id/merchant2Channel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView73"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="现有库存（条）"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"  />

            <TextView
                android:id="@+id/merchant2Remaining"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

            <TextView
                android:id="@+id/textView71"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="备注"
                android:textColor="@color/mb_red500"
                android:textSize="16sp"  />

            <TextView
                android:id="@+id/merchant2Comments"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/primary_darker"
                android:textSize="20sp"
                android:textStyle="bold"  />

        </LinearLayout>
    </ScrollView>
</RelativeLayout>
